}
.nav{
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	background: #f6f6f6;
	z-index: 999;
}
.nav:before{
	content: "";
	position: absolute;
	left: 0;
	background: #d8d8d8;
	width: 100%;
	height: 1px;
	-webkit-transform:scaleY(0.5);
	transform: scaleY(0.5);
	transform-origin: 0 0;
	-webkit-transform-origin:0 0;
}
.nav ul li{
	width: 25%;
	float: left;
}
.nav ul li a{
	text-align: center;
	padding-top: 28px;
	font-size: 1.4rem;
	color: #333;
	background: url(../Images/shangcheng.png) no-repeat 50% 25%;
