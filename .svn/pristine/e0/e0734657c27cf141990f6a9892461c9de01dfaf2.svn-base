body{
	font-family: "Microsoft Yahei" !important;
	/*background:#f8f8f8;*/
	background: #f3f3f3;
	height:100%;
	overflow:auto;
	margin: 0;
	-webkit-tap-highlight-color:rgba(0,0,0,0);
}
a{
	text-decoration: none;
}
body a{
	outline:none;
	blr:expression(this.onFocus=this.blur());
}
input{
	outline: none;
}
button{
	outline: none;
	cursor: pointer;
}
a:hover{
	text-decoration:none;
	outline:none;
}

a:focus{
	text-decoration:none;
	outline:none;
}

a:focus{
	outline:0;
}
button{
	font-size: 1.6rem;
}
ul li{
	list-style: none;
}
.left{margin-left: 3%;
	float: left;
}
.right{
	float: right;
}
.title{
	margin: auto;
	width: 100%;
	padding: 15px 0;
	background: #f35957;
	text-align: center;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 8;
	color: #fff;
	font-size: 1.6rem;
}
.title a{
	color: #fff;
}
.userCard{
	margin:48px 10px 10px;
