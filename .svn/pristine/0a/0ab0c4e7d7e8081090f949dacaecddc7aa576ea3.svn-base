/* reset */
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,table,th,td,iframe{margin:0;padding:0;}
body{font:17px Microsoft YaHei,Helvetica Neue,Helvetica,Roboto,STHeiTi,Arial,sans-serif;color:#3a3a3a;word-wrap:break-word;-webkit-text-size-adjust:none;}
table{border-collapse:collapse;border-spacing:0;}
img,header,nav,section,footer{display:block;}
abbr,acronym,fieldset,img,fieldset{border:0;}
ol,ul{list-style:none outside;}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal;}
address,caption,cite,code,dfn,em,strong,u,i,th,var{font-style:normal;} 
:focus{outline:0;}
q:before, q:after{content:'';}
abbr{cursor:help;}
a{text-decoration:none;}
a:hover{outline:none;}
.fl{float:left;display:inline;}
.fr{float:right;display:inline;}
.clearfix:after{content:".";height:0;visibility:hidden;display:block;clear:both;}
.clearfix{display:inline-block;}
.clearfix{display:block;}
.clear{height:0;overflow:hidden;clear:both;display:block;}
.o_h{overflow:hidden;}
.relative{position:relative;}
input,textarea{font-family:Microsoft YaHei,Helvetica Neue,Helvetica,Roboto,STHeiTi,Arial,sans-serif;}
img{width:100%;}

.picarr{ background: #000; width: 100%;padding:0;margin:0; overflow: hidden; }
.picarr li{width:100%; overflow: hidden; list-style:none;padding:0;margin:0; position:relative;}
.picarr li img{ width:100%;}
.picarr li a{ position:absolute; display:block; top:10px; right:10px; width:25px; height:25px; background:url(../img/delete.png) no-repeat;}

/* 公共样式 */
html{font-size:10px;}
header,section,footer,div,ul,nav{width:100vw;}
.pos_rela{position:relative;}
*{box-sizing:border-box;}
/* 首页样式 */
.title,.news,li{text-align:center;}
span{display:block;}
.title{width:100%;height:4rem;line-height:4rem;background:#de3d29;color:#fff;font-size:1.7rem;}
.banner img{width:100%;}
.news{height:3rem;line-height:3rem;background:#ccc;}
.s_l{font-weight:bold;font-size:1.3rem;padding-right:1rem;display:inline-block;}
.s_r{font-size:1.2rem;display:inline-block;}
.content{overflow:hidden;}
.content li{width:33%;text-align:center;float:left;}
.content li img{width:40%;display:inline-block;padding:1.5rem 0;}
.content li span{padding-bottom:1.5rem;font-size:1.3rem;}
.border_lr{border-left:.1rem #ddd solid;border-right:.1rem #ddd solid;}
.border_tb{border-top:.1rem #ddd solid;border-bottom:.1rem #ddd solid;}
footer{position:fixed;left:0;bottom:0;}
.menu{background:#f00;}
.menu li{width:50%;text-align:center;float:left;background:#b20405;}
.menu li img{width:20%;display:inline-block;padding:.5rem 0;}
.menu li span{padding-bottom:.5rem;color:#fff;font-size:1.1rem;}
/* 公示公告样式 */
.head_gg{text-align:center;background:#f35957;font-size:1.7rem;padding:1rem 0;color:#fff;}
.btnBack{width:5rem;text-align:center;position:absolute;left:1rem;top:1.2rem;color:#fff;font-size:1.5rem;}
.nav{width:100%;overflow:hidden;}
.nav li{width:33%;text-align:center;font-size:1.5rem;border-bottom:#bbb solid .1rem;float:left;padding:.7rem 0;}
.nav li:nth-child(2){border-left:.1rem #ccc solid;border-right:.1rem #bbb solid;}
.active{background:#eee;}
.cont_gg{width:100%;position:absolute;top:0;left:0;}
.cont_gg li{text-align:left;width:100%;padding:.7rem 1.5rem;border-bottom:.1rem solid #eee;font-size: 1.5rem;}
.pos_rela .cont_gg:not(:first-child){display:none;}
